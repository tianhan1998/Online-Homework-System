<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.j2.onlinehomeworksystem.mapper.IClassManageDaoMapper">
    <select id="selectAllClassGrade" resultType="Class">
        select * from c_grade
    </select>

    <select id="selectOneClassGrade" resultType="Class">
        select * from c_grade where classid=#{classid}
    </select>
</mapper>
